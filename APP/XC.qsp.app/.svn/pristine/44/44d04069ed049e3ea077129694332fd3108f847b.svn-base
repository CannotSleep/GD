
page {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.index {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  overflow: hidden;
  height: 100%;
}
.index-bd {
  width: 750rpx;
  height: 100%;
}
.swiper-tab {
  width: 100%;
  white-space: nowrap;
  line-height: 60rpx;
  height: 65rpx;
}
.swiper-tab-list {
  font-size: 30rpx;
  width: 50%;
  display: inline-block;
  text-align: center;
  color: #777777;
}
.on {
  color: #17A869;
  border-bottom: 5rpx solid #17A869;
}
.swiper-box {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  width: 100%;
  height: 1030rpx;
  min-height: 600rpx;
}
.tab-list {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: left;
  border-bottom: 2rpx solid #EFEFF4;
}
.data-row {
  width: 70%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
  justify-content: flex-end;
  padding: 15rpx 0;
}
.data-row ._button {
  color: #fff;
  background-color: #17A869;
  border: none;
  position: relative;
}